FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U_new;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];


internalField   uniform (2.4964 -3.16 0);

boundaryField
{
    movingWall
    {
        type            calculated;
        value           uniform (0.8 -0.8 0);
    }
    fixedWalls
    {
        type            calculated;
        value           uniform (0.8 -0.8 0);
    }
    frontAndBack
    {
        type            empty;
    }
}

